{
    "schemaVersion": "1.0.0",
    "props": {
        "pmi-state": {
            "type": "object"
        }
    },
    "actions":{
        "handlePMIChange": {
            "actionType": "JSFunction",
            "method": "updatePMIInfoView",
            "inputData": {
                "pmiState": "{{props.pmiState}}"
            },
            "outputData": {
                "resp":"resp"
            },
            "deps": "js/egorPmiPanelService"
        }
    },
    "lifecycleHooks": {
        "onUpdate": [
            {
                "action": "handlePMIChange",
                "observers": [
                    "props.pmiState"
                ]
            }
        ]
    }
}